{
  "extends": [
    "next/core-web-vitals",
    "prettier"
  ],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error",
    "react/jsx-max-props-per-line": [1, { "maximum": 1, "when": "multiline" }],
    "react/jsx-first-prop-new-line": [1, "multiline"],
    "react/jsx-closing-bracket-location": [1, "line-aligned"]
  }
}
